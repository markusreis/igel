{
  "name": "igel-master",
  "version": "1.0.0",
  "description": "parcel bundler for igel admin stuff",
  "main": "index.js",
  "author": "yl",
  "license": "MIT",
  "scripts": {
    "start": "parcel ./index.js",
    "build": "parcel build ./src/index.js --out-dir ./dist/ -o igel-master.bundle.min.js --no-source-maps --experimental-scope-hoisting",
    "watch": "parcel watch ./src/index.js --out-dir ./dist/ -o igel-master.bundle.min.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "cssnano": "^4.1.8",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-dist": "0.0.6",
    "sass": "^1.29.0",
    "terser": "^5.3.8"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "autoprefixer": "^9.4.6",
    "postcss-modules": "^1.4.1"
  }
}
